{
  "name": "archive-app",
  "version": "1.0.0",
  "description": "Helps design-nerds find popular themes in design",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test/*.js | node | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{css,js,html} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push",
    "watch": "watchify . -o bundle.js -g uglifyify"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katenorquay/archive-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/katenorquay/archive-app/issues"
  },
  "homepage": "https://github.com/katenorquay/archive-app#readme",
  "dependencies": {
    "clone": "^2.1.0",
    "deep-freeze": "0.0.1",
    "redux": "^3.6.0",
    "sheet-router": "^4.1.2",
    "superagent": "^3.1.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}
